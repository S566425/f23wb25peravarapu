extends layout

block content
    div(class="container-fluid")
    h1= title
    - let results=[{course:"Web apps",faculty:"Chandra Mouli",section:6},{course:"Java",faculty:"Ajay Bandi",section:5},{course:"ADB",faculty:"Qin ",section:4}]
    div(class="row")
        each form,item in results
            div(class="col-sm-2") #{form.course}
            div(class="col-sm-2") #{form.faculty}
            div(class="col-sm-2") #{form.section}
            
